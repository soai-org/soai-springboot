<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.team1.soai.mapper.UserMapper">
    <select id="getUserInfo" resultType="com.team1.soai.dto.UserDTO">
        SELECT
             user_id
            , user_name
            , user_password
            , user_role
            , created_at
            , updated_at
        FROM USER
        WHERE user_id = #{userId}
    </select>


</mapper>